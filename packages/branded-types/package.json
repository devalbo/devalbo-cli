{
  "name": "@devalbo-cli/branded-types",
  "version": "0.2.5",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "type-check": "tsc -p tsconfig.json --noEmit",
    "test:types": "tsc -p tsconfig.test.json --noEmit",
    "test": "npm run test:types && vitest run tests",
    "prepare": "npm run build"
  },
  "dependencies": {
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "vitest": "^4.0.18"
  }
}

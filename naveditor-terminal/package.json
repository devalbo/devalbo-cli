{
  "name": "naveditor-terminal",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "build": "echo 'naveditor-terminal: no build step required'",
    "terminal": "node --import tsx ../naveditor-lib/src/cli-node.tsx",
    "interactive": "node --import tsx ../naveditor-lib/src/cli-node.tsx interactive",
    "terminal:dev": "node --import tsx ../naveditor-lib/src/cli-node.tsx",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@devalbo/naveditor-lib": "workspace:*",
    "@devalbo/commands": "workspace:*",
    "@devalbo/filesystem": "workspace:*",
    "@devalbo/shared": "workspace:*",
    "@devalbo/state": "workspace:*",
    "@devalbo/ui": "workspace:*",
    "commander": "catalog:",
    "effect": "catalog:",
    "ink": "catalog:",
    "ink-select-input": "catalog:",
    "react": "catalog:",
    "zod": "catalog:"
  },
  "devDependencies": {
    "@types/node": "catalog:",
    "@types/react": "catalog:",
    "tsx": "catalog:",
    "typescript": "catalog:"
  }
}

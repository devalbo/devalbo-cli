{
  "name": "devalbo-editor-workspace",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "workspaces": [
    "packages/*",
    "apps/*"
  ],
  "scripts": {
    "type-check": "npm run type-check --workspaces --if-present",
    "test": "npm run test --workspaces --if-present",
    "test:workspace": "vitest run",
    "build": "npm run build --workspaces --if-present",
    "web:dev": "npm run dev --workspace=naveditor-web",
    "web:build": "npm run build --workspace=naveditor-web",
    "terminal": "npm run terminal --workspace=naveditor-terminal",
    "terminal:interactive": "npm run interactive --workspace=naveditor-terminal",
    "terminal:dev": "npm run terminal:dev --workspace=naveditor-terminal",
    "desktop:dev": "npm run dev --workspace=naveditor-desktop",
    "desktop:build": "npm run build --workspace=naveditor-desktop"
  },
  "devDependencies": {
    "vitest": "^4.0.18"
  }
}
